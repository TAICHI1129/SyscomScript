{
  "name": "syscomscript",
  "displayName": "SyscomScript",
  "description": "SyscomScript language support for VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": ["Programming Languages"],
  "contributes": {
    "languages": [
      {
        "id": "syscom",
        "aliases": ["SyscomScript", "syscom"],
        "extensions": [".scs"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "syscom",
        "scopeName": "source.syscom",
        "path": "./syntaxes/syscom.tmLanguage.json"
      }
    ]
  },
  "activationEvents": ["onLanguage:syscom"],
  "main": "./extension.js"
}
